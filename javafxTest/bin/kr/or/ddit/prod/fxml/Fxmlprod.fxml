<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="300.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="kr.or.ddit.prod.main.ControllerProd">
   <children>
      <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0">
         <children>
            <ComboBox fx:id="LprodBox" onAction="#lprodser" prefWidth="180.0">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </ComboBox>
            <ComboBox fx:id="ProdBox" onAction="#prodser" prefWidth="350.0">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </ComboBox>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <TableView fx:id="TableView" prefHeight="200.0" prefWidth="700.0">
        <columns>
          <TableColumn fx:id="Prod_ID" prefWidth="75.0" text="Prod_ID" />
          <TableColumn fx:id="Prod_Name" prefWidth="75.0" text="Prod_Name" />
            <TableColumn fx:id="Prod_LGU" prefWidth="75.0" text="Prod_LGU" />
            <TableColumn fx:id="Prod_Buyer" prefWidth="75.0" text="Prod_Buyer" />
            <TableColumn fx:id="Prod_Cost" prefWidth="75.0" text="Prod_Cost" />
            <TableColumn fx:id="Prod_Price" prefWidth="75.0" text="Prod_Price" />
            <TableColumn fx:id="Prod_Sale" prefWidth="75.0" text="Prod_Sale" />
            <TableColumn fx:id="Prod_OutLine" prefWidth="113.0" text="Prod_OutLine" />
            <TableColumn fx:id="Prod_Detail" prefWidth="141.0" text="Prod_Detail" />
        </columns>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </TableView>
   </children>
</VBox>
